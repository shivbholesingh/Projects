# Magic methods in python


class person:

    def __init__(self, name, age):
        self.name = name
        self.age = age

    def __str__(self):
      return f"{self.name}({self.age})"

    def __repr__(self):
        return f"person('{self.name}',{self.age})"

    def __call__(self):
        print("Hey I am good")


p = person("Harry", 30)
print(p)

print(p.__str__)


